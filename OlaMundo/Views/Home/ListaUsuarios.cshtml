@model List<MinhaAplicacao.Models.Usuario>

<h2>Lista de Usu√°rios</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Telefone</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <td>@usuario.Id</td>
                <td>@usuario.Nome</td>
                <td>@usuario.Telefone</td>
                <td>@usuario.Email</td>
            </tr>
        }
    </tbody>
</table>
